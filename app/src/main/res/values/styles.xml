<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="colorPrimary">@color/colorPrimary_default</item>
		<item name="colorPrimaryDark">@color/colorPrimaryDark_default</item>
		<item name="colorAccent">@color/colorAccent_default</item>
	</style>

	<style name="AppTheme.NoActionBar">
		<item name="windowActionBar">false</item>
		<item name="windowNoTitle">true</item>
	</style>

	<style name="AppTheme.NoActionBar.Blue">
		<item name="colorPrimary">@color/blue_500</item>
		<item name="colorPrimaryDark">@color/blue_700</item>
		<item name="colorAccent">@color/red_700</item>
	</style>

	<style name="AppTheme.NoActionBar.Brown">
		<item name="colorPrimary">@color/brown_500</item>
		<item name="colorPrimaryDark">@color/brown_700</item>
		<item name="colorAccent">@color/lime_A100</item>
	</style>

	<style name="AppTheme.NoActionBar.Green">
		<item name="colorPrimary">@color/green_500</item>
		<item name="colorPrimaryDark">@color/green_700</item>
		<item name="colorAccent">@color/orange_A400</item>
	</style>

	<style name="AppTheme.NoActionBar.Lime">
		<item name="colorPrimary">@color/lime_500</item>
		<item name="colorPrimaryDark">@color/lime_700</item>
		<item name="colorAccent">@color/blue_A700</item>
	</style>

	<style name="AppTheme.NoActionBar.Red">
		<item name="colorPrimary">@color/red_500</item>
		<item name="colorPrimaryDark">@color/red_700</item>
		<item name="colorAccent">@color/lime_A100</item>
	</style>

	<style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

	<style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

	<style name="AppTheme.NoActionBar.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

	<style name="AppTheme.NoActionBar.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

	<style name="ButtonStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_margin">16dp</item>
		<item name="android:background">@drawable/button_rounded_rectangle</item>
		<item name="android:clickable">true</item>
		<item name="android:focusable">true</item>
		<item name="android:textColor">@android:color/white</item>
	</style>

	<style name="ButtonStyle.Small">
		<item name="android:layout_width">40dp</item>
		<item name="android:layout_height">40dp</item>
		<item name="android:scaleType">fitCenter</item>
		<item name="android:layout_margin">2dp</item>
		<item name="android:paddingStart">9dp</item>
		<item name="android:background">@drawable/button_rounded_rectangle_small_selector</item>
	</style>

	<style name="ButtonStyle.Medium">
		<item name="android:layout_height">50dp</item>
		<item name="android:layout_marginStart">15dp</item>
		<item name="android:scaleType">fitCenter</item>
		<item name="android:textSize">10sp</item>
		<item name="android:textAllCaps">true</item>
		<item name="android:tint">@color/colorPrimaryDark_default</item>
		<item name="android:background">@drawable/button_rounded_rectangle_medium_selector</item>
		<item name="android:gravity">center_horizontal|bottom</item>
		<item name="android:textColor">@color/colorPrimaryDark_default</item>
	</style>

	<style name="ButtonStyle.Medium.Transparent">
		<item name="android:background">@drawable/button_rounded_rectangle_medium_selector_trans
		</item>
	</style>

	<style name="ButtonStyle.Submit">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">0dp</item>
		<item name="android:layout_margin">6dp</item>
		<item name="android:background">#80000000</item>
		<item name="android:textColor">@color/lime_A100</item>
		<item name="android:radius">2dp</item>
		<item name="android:textAllCaps">true</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textSize">14sp</item>
	</style>

	<style name="ButtonStyle.Round">
		<item name="android:padding">0dp</item>
		<item name="android:layout_margin">@dimen/button_margin_round</item>
		<item name="android:layout_width">@dimen/round_button_size</item>
		<item name="android:layout_height">@dimen/round_button_size</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textStyle">normal</item>
		<item name="android:background">@drawable/button_round_normal_selector</item>
	</style>

	<style name="ButtonStyle.Round.Wicket">
		<item name="android:background">@drawable/button_round_wicket_selector</item>
	</style>

	<style name="ButtonStyle.Round.Boundary">
		<item name="android:background">@drawable/button_round_boundary_selector</item>
	</style>

	<style name="ButtonStyle.Round.Extras">
		<item name="android:background">@drawable/button_round_extra_selector</item>
	</style>

	<style name="TextViewStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">4dp</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">@color/colorPrimaryDark_default</item>
	</style>

	<style name="TextViewStyle.MorePadding">
		<item name="android:padding">12dp</item>
	</style>

	<style name="TextViewStyle.LeftRightPadding">
		<item name="android:paddingStart">12dp</item>
		<item name="android:paddingEnd">12dp</item>
	</style>

	<style name="TextViewStyle.List">
		<item name="android:padding">10dp</item>
	</style>

	<style name="TextViewStyle.List.Bold">
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TextViewStyle.Bold">
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TextViewStyle.Bold.Medium">
		<item name="android:textSize">14sp</item>
	</style>

	<style name="TextViewStyle.Medium">
		<item name="android:textSize">14sp</item>
	</style>

	<style name="TextViewStyle.Medium.MorePadding">
		<item name="android:padding">12dp</item>
	</style>

	<style name="TextViewStyle.Bold.Medium.MorePadding">
		<item name="android:padding">12dp</item>
	</style>

	<style name="TextViewStyle.BigMargin">
		<item name="android:layout_marginStart">12dp</item>
		<item name="android:layout_marginEnd">12dp</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_marginBottom">20dp</item>
	</style>

	<style name="TextViewStyle.Big">
		<item name="android:textSize">16sp</item>
	</style>

	<style name="TextViewStyle.Big.BigMargin">
		<item name="android:layout_marginStart">12dp</item>
		<item name="android:layout_marginEnd">12dp</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_marginBottom">20dp</item>
	</style>

	<style name="TextViewStyle.Big.Bold">
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TextViewStyle.Big.Bold.SmallMargin">
		<item name="android:layout_marginStart">4dp</item>
		<item name="android:layout_marginEnd">4dp</item>
		<item name="android:layout_marginTop">10dp</item>
		<item name="android:layout_marginBottom">10dp</item>
	</style>

	<style name="TextViewStyle.Big.Bold.BigMargin">
		<item name="android:layout_marginStart">12dp</item>
		<item name="android:layout_marginEnd">12dp</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_marginBottom">20dp</item>
	</style>

	<style name="TextViewStyle.Big.Bold.Heading">
		<item name="android:layout_width">match_parent</item>
		<item name="android:textAllCaps">true</item>
		<item name="android:paddingTop">10dp</item>
		<item name="android:paddingBottom">10dp</item>
		<item name="android:background">@color/gray_500</item>
	</style>

	<style name="TextViewStyle.Big.Bold.Highlight">
		<item name="android:textColor">@color/orange_600</item>
	</style>

	<style name="TextViewStyle.Large">
		<item name="android:textSize">20sp</item>
	</style>

	<style name="TextViewStyle.Large.Bold">
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TextViewStyle.Large.BigMargin">
		<item name="android:layout_marginStart">12dp</item>
		<item name="android:layout_marginEnd">12dp</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_marginBottom">20dp</item>
	</style>

	<style name="TextViewStyle.Large.Bold.BigMargin">
		<item name="android:layout_marginStart">12dp</item>
		<item name="android:layout_marginEnd">12dp</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_marginBottom">20dp</item>
	</style>

	<style name="TextViewStyle.TableRow">
		<item name="android:padding">4dp</item>
	</style>

	<style name="TextViewStyle.TableRow.Header">
		<item name="android:textSize">12sp</item>
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TextViewStyle.TableRow.Left">
		<item name="android:textSize">12sp</item>
		<item name="android:textStyle">bold</item>
	</style>

	<style name="TextViewStyle.TableRow.Center">
		<item name="android:background">@drawable/borders_inner</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="TextViewStyle.TableRow.Header.Center">
		<item name="android:background">@drawable/borders_inner</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="TextViewStyle.TableRow.Header.Border">
		<item name="android:background">@drawable/borders_inner</item>
		<item name="android:gravity">center</item>
	</style>

	<style name="TextViewStyle.TableRow.Left.Border">
		<item name="android:background">@drawable/borders_inner</item>
		<item name="android:gravity">center_vertical</item>
	</style>

	<style name="TextViewStyle.TableRow.Data">
		<item name="android:layout_width">30dp</item>
		<item name="android:gravity">right</item>
	</style>

	<style name="TextViewStyle.TableRow.Data.Header">
		<item name="android:textSize">12sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:gravity">right</item>
	</style>

	<style name="EditTextStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:padding">8dp</item>
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">@color/colorPrimaryDark_default</item>
	</style>

	<style name="EditTextStyle.Medium">
		<item name="android:textSize">14sp</item>
	</style>

	<style name="RadioButtonStyle">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">12sp</item>
		<item name="android:padding">10dp</item>
	</style>

	<style name="RadioButtonStyle.SmallPadding">
		<item name="android:padding">4dp</item>
	</style>

	<style name="RadioButtonStyle.NoPadding">
		<item name="android:padding">0dp</item>
	</style>

	<style name="LineSeparator">
		<item name="android:layout_height">1dip</item>
		<item name="android:layout_width">match_parent</item>
		<item name="android:background">@color/lineSeparator</item>
	</style>

	<style name="LineSeparator.Dark">
		<item name="android:background">@color/teal_A100</item>
	</style>

	<style name="LineSeparator.PrimaryDark">
		<item name="android:background">@color/colorPrimaryDark_default</item>
	</style>

	<style name="LineSeparator.Dark.SmallMargin">
		<item name="android:background">@color/teal_A100</item>
		<item name="android:layout_marginTop">4dp</item>
		<item name="android:layout_marginBottom">4dp</item>
	</style>

	<style name="LineSeparator.Dark.Margin">
		<item name="android:background">@color/teal_A100</item>
		<item name="android:layout_marginTop">20dp</item>
		<item name="android:layout_marginBottom">12dp</item>
	</style>

	<style name="ImageButtonStyle">
		<item name="android:layout_width">60dp</item>
		<item name="android:layout_height">60dp</item>
		<item name="android:layout_marginTop">10dp</item>
		<item name="android:layout_marginStart">20dp</item>
		<item name="android:scaleType">fitCenter</item>
		<item name="android:background">@android:color/transparent</item>
		<item name="android:textSize">10sp</item>
	</style>

	<style name="ImageButtonStyle.Runs">
		<item name="android:background">@drawable/shape_rectangle_runs</item>
	</style>

	<style name="ImageButtonStyle.Extras">
		<item name="android:background">@drawable/shape_rectangle_extras</item>
	</style>

	<style name="ImageButtonStyle.NoText">
		<item name="android:layout_width">24dp</item>
		<item name="android:layout_height">24dp</item>
		<item name="android:padding">4dp</item>
		<item name="android:layout_margin">4dp</item>
	</style>

	<style name="LinearLayoutStyleSubmit">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:background">#60000000</item>
		<item name="android:gravity">end</item>
		<item name="android:orientation">horizontal</item>
		<item name="android:radius">1dp</item>
		<item name="android:layout_alignParentEnd">true</item>
	</style>

	<style name="LinearLayoutItemView">
		<item name="android:layout_width">match_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingTop">8dp</item>
		<item name="android:paddingBottom">8dp</item>
		<item name="android:orientation">horizontal</item>
	</style>

	<style name="ImageViewPartnership">
		<item name="android:layout_width">1dp</item>
		<item name="android:layout_height">20dp</item>
		<item name="android:padding">0dp</item>
		<item name="android:layout_margin">0dp</item>
	</style>

	<style name="FullscreenTheme" parent="AppTheme.NoActionBar">
		<item name="android:actionBarStyle">@style/FullscreenActionBarStyle</item>
		<item name="android:windowActionBarOverlay">true</item>
		<item name="android:windowBackground">@null</item>
		<item name="metaButtonBarStyle">?android:attr/buttonBarStyle</item>
		<item name="metaButtonBarButtonStyle">?android:attr/buttonBarButtonStyle</item>
	</style>

	<style name="FullscreenActionBarStyle" parent="Widget.AppCompat.ActionBar">
		<item name="android:background">@color/black_overlay</item>
	</style>
</resources>